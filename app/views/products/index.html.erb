        <script src="/cn/store/js/lib/fastclick.js"></script>

        <script src="/cn/store/js/muji-1.0.js"></script>
        <script src="/cn/store/js/portal-2.0.js"></script>
        <script src="/cn/store/js/portal_cn.js"></script>

        <script>
            //FastClick
            if (cssua.ua.mobile) {
                $(function () {
                    var selectors = [
                        $("#hGlobalNav h1").get(0),
                        $("#hSearch h1").get(0),
                        $(".cancel").get(0),
                        $(".prev").get(0),
                        $(".next").get(0),
                        $(".avater").get(0)
                    ];

                    for (var i = 0, sLen = selectors.length; i < sLen; i++) {
                        if (selectors[i] != undefined) {
                            FastClick.attach(selectors[i]);
                        }
                    }
                });
            }
        </script>

        <script type="text/javascript" src="http://api.map.baidu.com/api?v=1.5&ak=18c2abab0deabc84cf240d2e1123f5a1"></script>
